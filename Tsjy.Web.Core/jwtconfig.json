{
  "JWTSettings": {
    // 是否验证密钥，bool 类型，默认true
    "ValidateIssuerSigningKey": true,
    // 密钥，string 类型，必须是复杂密钥，长度大于16，.NET8+ 长度需大于 32
    "IssuerSigningKey": "abcd123409878765abcd123409878765",
    // 是否验证签发方，bool 类型，默认true
    "ValidateIssuer": true,
    // 签发方，string 类型
    "ValidIssuer": "hdxySoft",
    // 是否验证签收方，bool 类型，默认true
    "ValidateAudience": true,
    // 签收方，string 类型
    "ValidAudience": "SoftHdxy",
    // 是否验证过期时间，bool 类型，默认true，建议true
    "ValidateLifetime": true,
    // 过期时间，long 类型，单位分钟，默认20分钟，最大支持 13 年
    "ExpiredTime": 20,
    // 过期时间容错值，long 类型，单位秒，默认 5秒
    "ClockSkew": 5,
    // 加密算法，string 类型，默认 HS256
    "Algorithm": "HS256",
    // 验证过期时间，设置 false 将永不过期，Furion 4.9.1.46+ 支持
    "RequireExpirationTime": true
  }
}